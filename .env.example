# Environment Variables Template (.env) for Docker

# Required - Supabase Configuration
SUPABASE_URL=
SUPABASE_KEY=

# Optional - Environment
NODE_ENV=production

# Optional - Performance Tuning
MAX_CONCURRENCY=3
LOOP_INTERVAL_MS=40000
TIMEOUT_MS=60000
MAX_FAILS=2
BLOCK_OFFLINE_MS=600000
RETRY_LIMIT=2
MAX_BATCH_SIZE=50
MAX_BATCH_BUFFER=200
CACHE_TTL_MS=30000
MIN_UPDATE_INTERVAL_MS=30000
VIEWERS_CHANGE_THRESHOLD=0

# App Port (container)
PORT=3000
